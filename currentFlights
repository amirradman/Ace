import javax.swing.*;
import java.awt.*;
import java.awt.event.*;

public class currentFlights extends JFrame 
{
	public static void main(String[] args)
	{
		EventQueue.invokeLater(new Runnable() 
		{
			public void run()
			{
				currentUsers frame = new currentUsers();
				frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
				frame.setVisible(true);
			}
		});
	}
	
	public currentFlights()
	{
		setTitle("Current Flights");
		setSize(DEFAULT_WIDTH, DEFAULT_HEIGHT);
		
		//add panel to frame
		currentFlightsPanel panel = new currentFlightsPanel();
		add(panel);
	}
	
	public static final int DEFAULT_WIDTH = 700;
	public static final int DEFAULT_HEIGHT = 600;

}

class currentFlightsPanel extends JPanel
{
	public void paintComponent(Graphics g)
	{
		
	}
}
